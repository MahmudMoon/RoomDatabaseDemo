<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.HomeFragment">


    <Button
        android:id="@+id/add_user"
        android:layout_width="151dp"
        android:layout_height="0dp"
        android:layout_marginTop="167dp"
        android:layout_marginBottom="41dp"
        android:text="add_user"
        app:layout_constraintBottom_toTopOf="@+id/get_user"
        app:layout_constraintStart_toStartOf="@+id/get_user"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/get_user"
        android:layout_width="159dp"
        android:layout_height="0dp"
        android:layout_marginEnd="111dp"
        android:layout_marginBottom="39dp"
        android:text="get_user"
        app:layout_constraintBottom_toTopOf="@+id/remove_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_user" />

    <Button
        android:id="@+id/remove_user"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="40dp"
        android:text="remove_user"
        app:layout_constraintBottom_toTopOf="@+id/Update_user"
        app:layout_constraintEnd_toEndOf="@+id/get_user"
        app:layout_constraintStart_toStartOf="@+id/get_user"
        app:layout_constraintTop_toBottomOf="@+id/get_user" />

    <Button
        android:id="@+id/Update_user"
        android:layout_width="162dp"
        android:layout_height="0dp"
        android:layout_marginBottom="208dp"
        android:text="Update_user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/remove_user"
        app:layout_constraintTop_toBottomOf="@+id/remove_user" />
</androidx.constraintlayout.widget.ConstraintLayout>